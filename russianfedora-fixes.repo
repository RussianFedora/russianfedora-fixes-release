[russianfedora-fixes]
name=Russian Fedora for Fedora $releasever - Fixes
#baseurl=http://mirror.yandex.ru/fedora/russianfedora/russianfedora/fixes/fedora/releases/$releasever/Everything/$basearch/os
mirrorlist=http://mirrors.rfremix.ru/mirrorlist?repo=fixes-fedora-$releasever&arch=$basearch
enabled=0
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-russianfedora-fixes-fedora

[russianfedora-fixes-debuginfo]
name=Russian Fedora for Fedora $releasever - Fixes - Debug
#baseurl=http://mirror.yandex.ru/fedora/russianfedora/russianfedora/fixes/fedora/releases/$releasever/Everything/$basearch/debug
mirrorlist=http://mirrors.rfremix.ru/mirrorlist?repo=fixes-fedora-debug-$releasever&arch=$basearch
enabled=0
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-russianfedora-fixes-fedora

[russianfedora-fixes-source]
name=Russian Fedora for Fedora $releasever - Fixes - Source
#baseurl=http://mirror.yandex.ru/fedora/russianfedora/russianfedora/fixes/fedora/releases/$releasever/Everything/source/SRPMS
mirrorlist=http://mirrors.rfremix.ru/mirrorlist?repo=fixes-fedora-source-$releasever&arch=$basearch
enabled=0
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-russianfedora-fixes-fedora

