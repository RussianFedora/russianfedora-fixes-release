[russianfedora-fixes-updates]
name=Russian Fedora for EL $releasever - Fixes - Updates
#baseurl=http://mirror.yandex.ru/fedora/russianfedora/russianfedora/fixes/el/updates/$releasever/$basearch
mirrorlist=http://mirrors.rfremix.ru/mirrorlist?repo=fixes-reremix-updates-released-$releasever&arch=$basearch
exclude=reremix*
enabled=1
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-russianfedora-fixes-fedora

[russianfedora-fixes-updates-debuginfo]
name=Russian Fedora for EL $releasever - Fixes - Updates Debug
#baseurl=http://mirror.yandex.ru/fedora/russianfedora/russianfedora/fixes/el/updates/$releasever/$basearch/debug
mirrorlist=http://mirrors.rfremix.ru/mirrorlist?repo=fixes-reremix-updates-released-debug-$releasever&arch=$basearch
enabled=0
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-russianfedora-fixes-fedora

[russianfedora-fixes-updates-source]
name=Russian Fedora for EL $releasever - Fixes - Updates Source
#baseurl=http://mirror.yandex.ru/fedora/russianfedora/russianfedora/fixes/el/updates/$releasever/SRPMS
mirrorlist=http://mirrors.rfremix.ru/mirrorlist?repo=fixes-reremix-updates-released-source-$releasever&arch=$basearch
enabled=0
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-russianfedora-fixes-fedora

